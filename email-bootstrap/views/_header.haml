- generic = true
%tr
  %td{td_attrs(:align => 'center',  :role => 'banner')}
    %table{table_attrs(:width => $fix_width)}
      - unless generic
        - 2.times do
          %tr
            = partial :'modules/_toc'
      - else
        %tr
          %td{center, middle, colspan (3)}
            %img{img_attrs(:width => $fix_width, :src => "http://www.partypoker.com/images/1/newsletter/main.jpg")}
        %tr
          = partial :'modules/_toc'
   

