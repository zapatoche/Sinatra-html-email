%table.brand{table_attrs(bgcolor: $wrapper_bg, width: $fix_width)}
  %tr
    %td{td_attrs, bgcolor: $wrapper_bg, height: "5"}
      %img{src: "#{$img_dev_url}spacer.gif", height: "5", width: "1", style: "display: block;"}
  %tr
    %td{td_attrs, bgcolor: $brand_color, width: $fix_width}
      %a{link_attrs}
        %img{img_attrs(src: "#{$img_dev_url}brand-logo.jpg", alt: "www.partypoker.com")}/
  %tr
    %td{td_attrs, bgcolor: $brand_highlight_secondary, height: "5"}
  -# %tr
    %td{td_attrs, bgcolor: $brand_highlight_primary, height: "1"}
      %img{src: "#{$img_dev_url}spacer.gif", height: "1", width: "1", style: "display: block;"}
